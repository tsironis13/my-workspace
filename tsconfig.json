{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@business-portal/backend": [
        "libs/backend/business-portal/src/public-api.ts"
      ],
      "@business-portal/frontend": [
        "libs/frontend/business-portal/src/public-api.ts"
      ],
      "@business-portal/core/config": [
        "projects/business-portal/src/app/core/core.config.ts"
      ],
      "@business-portal/ui": [
        "projects/business-portal/src/app/ui/public-api.ts"
      ],
      "@business-portal/core/entities/application": [
        "projects/business-portal/src/app/core/entities/application/public-api.ts"
      ],
      "@business-portal/core/entities/infrastructure": [
        "projects/business-portal/src/app/core/entities/infrastructure/public-api.ts"
      ],
      "@business-portal/administration/user-management/domain": [
        "projects/business-portal/src/app/feature/administration/user-management-subfeature/domain/public-api.ts"
      ],
      "@business-portal/administration/user-assignments/domain": [
        "projects/business-portal/src/app/feature/administration/user-assignments-subfeature/domain/public-api.ts"
      ]
    },
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022"
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
