{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@business-portal/env": [
        "projects/business-portal/src/environments/environment.ts"
      ],
      "@business-portal/backend": [
        "libs/backend/business-portal/src/public-api.ts"
      ],
      "@business-portal/frontend": [
        "libs/frontend/business-portal/src/public-api.ts"
      ],
      "@business-portal/core/config": [
        "projects/business-portal/src/app/core/config/public-api.ts"
      ],
      "@business-portal/core/auth": [
        "projects/business-portal/src/app/core/auth/public-api.ts"
      ],
      "@business-portal/core/navigation": [
        "projects/business-portal/src/app/core/navigation/public-api.ts"
      ],
      "@business-portal/core/utils": [
        "projects/business-portal/src/app/core/utils/public-api.ts"
      ],
      "@business-portal/core/business-groups": [
        "projects/business-portal/src/app/core/business-groups/public-api.ts"
      ],
      "@business-portal/core/business-entity-types": [
        "projects/business-portal/src/app/core/business-entity-types/public-api.ts"
      ],
      "@business-portal/core/store": [
        "projects/business-portal/src/app/core/store/public-api.ts"
      ],
      "@business-portal/ui": [
        "projects/business-portal/src/app/ui/public-api.ts"
      ],
      "@business-portal/pattern": [
        "projects/business-portal/src/app/pattern/public-api.ts"
      ],
      "@business-portal/core/entities/application": [
        "projects/business-portal/src/app/core/entities/application/public-api.ts"
      ],
      "@business-portal/core/entities/infrastructure": [
        "projects/business-portal/src/app/core/entities/infrastructure/public-api.ts"
      ],
      "@business-portal/access-control/presentation": [
        "projects/business-portal/src/app/domains/access-control/presentation/public-api.ts"
      ],
      "@business-portal/access-control/data": [
        "projects/business-portal/src/app/domains/access-control/data/public-api.ts"
      ],
      "@business-portal/access-control/infrastructure": [
        "projects/business-portal/src/app/domains/access-control/infrastructure/public-api.ts"
      ],
      "@business-portal/user-assignments/infrastructure": [
        "projects/business-portal/src/app/domains/user-assignments/infrastructure/public-api.ts"
      ],
      "@business-portal/user-assignments/data": [
        "projects/business-portal/src/app/domains/user-assignments/data/public-api.ts"
      ],
      "@business-portal/user-assignments/presentation": [
        "projects/business-portal/src/app/domains/user-assignments/presentation/public-api.ts"
      ],
      "@shared/auth": ["libs/shared/auth/src/public-api.ts"],
      "@shared/forms": ["libs/shared/forms/src/public-api.ts"],
      "@shared/global-loader": ["libs/shared/global-loader/src/public-api.ts"],
      "@shared/toast": ["libs/shared/toast/src/public-api.ts"]
    },
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022"
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
